{
    "manifest_version": 2,
    "name": "Smudge",
    "version": "1.0",
    "description": "Smudges your Fingerprint!",
    "permissions": [
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ],
    "background": {
        "scripts": ["back.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_start"
        }
    ],
    "browser_action": {
        "default_popup": "index.html"
    }
}